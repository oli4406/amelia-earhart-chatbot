{
  "source": {
    "include": [
      "BackEnd/server.js",
      "BackEnd/config",
      "BackEnd/services",
      "BackEnd/routes",
      "BackEnd/models",
      "BackEnd/middleware",
      "FrontEnd/src"
    ],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(node_modules|test)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/jsdoc/templates/default",
    "destination": "./docs/amelia-earhart-chatbot/0.0.0",
    "recurse": true,
    "readme": "./README.md"
  }
}